{
  "name": "mind-flow",
  "productName": "mind-flow",
  "version": "1.0.0",
  "description": "Final Year Project",
  "main": ".webpack/main",
  "scripts": {
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "start": "electron-forge start",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": {
    "name": "Teh Zhen Rong",
    "email": "zhenrong1999@student.usm.my"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "mind_flow"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/Electron/index.html",
                  "js": "./src/Electron/renderer.ts",
                  "preload": {
                    "js": "./src/Electron/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "6.0.0-beta.64",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^28.1.2",
    "@types/katex": "^0.14.0",
    "@types/lodash": "^4.14.182",
    "@types/module-alias": "^2.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-pdf": "^5.7.2",
    "@types/react-scroll": "^1",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "^19.0.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fake-indexeddb": "^3.1.8",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "jest": "^28.1.1",
    "node-loader": "^2.0.0",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "~4.7.4"
  },
  "dependencies": {
    "@antv/g6": "^4.6.6",
    "@fluentui/react-northstar": "^0.63.1",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@uiw/react-md-editor": "^3.14.4",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.2",
    "dexie-export-import": "^1.0.3",
    "dexie-react-hooks": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "file-loader": "^6.2.0",
    "katex": "^0.16.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "path-browserify": "^1.0.1",
    "pdf.js-extract": "^0.2.0",
    "pdfjs-dist": "^2.14.305",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pdf": "^5.7.2",
    "react-scroll": "^1.8.7",
    "webpack": "^5.73.0"
  },
  "packageManager": "yarn@3.2.0"
}
